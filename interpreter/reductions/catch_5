| x -> k x y
 in f' \ z. handler{hExcept} * \ zs. do z <- (Fst zs
 in do s' <- (Snd zs
 in do k' <- (\ z. handler{hInc} * Return z z
 in k' s'))) z))

77.
{-- E-Do and E-Hand and E-AppAbs --}

do y <- (handler{hExcept} * Return ("fail", 9)
 in do f' <- (\ z. Return \ k. case z of
 e -> Return Left e 
| x -> k x y
 in f' \ z. handler{hExcept} * \ zs. do z <- (Fst zs
 in do s' <- (Snd zs
 in do k' <- (\ z. handler{hInc} * Return z z
 in k' s'))) z))

78.
{-- E-Do and E-HandRet --}

do y <- (Return Right ("fail", 9)
 in do f' <- (\ z. Return \ k. case z of
 e -> Return Left e 
| x -> k x y
 in f' \ z. handler{hExcept} * \ zs. do z <- (Fst zs
 in do s' <- (Snd zs
 in do k' <- (\ z. handler{hInc} * Return z z
 in k' s'))) z))

79.
{-- E-DoRet --}

do f' <- (\ z. Return \ k. case z of
 e -> Return Left e 
| x -> k x Right ("fail", 9)
 in f' \ z. handler{hExcept} * \ zs. do z <- (Fst zs
 in do s' <- (Snd zs
 in do k' <- (\ z. handler{hInc} * Return z z
 in k' s'))) z)

80.
{-- E-Do and E-AppAbs --}

do f' <- (Return \ k. case Right ("fail", 9) of
 e -> Return Left e 
| x -> k x
 in f' \ z. handler{hExcept} * \ zs. do z <- (Fst zs
 in do s' <- (Snd zs
 in do k' <- (\ z. handler{hInc} * Return z z
 in k' s'))) z)

81.
{-- E-DoRet --}

\ k. case Right ("fail", 9) of
 e -> Return Left e 
| x -> k x \ z. handler{hExcept} * \ zs. do z <- (Fst zs
 in do s' <- (Snd zs
 in do k' <- (\ z. handler{hInc} * Return z z
 in k' s'))) z

82.
{-- E-AppAbs --}

case Right ("fail", 9) of
 e -> Return Left e 
| x -> \ z. handler{hExcept} * \ zs. do z <- (Fst zs
 in do s' <- (Snd zs
 in do k' <- (\ z. handler{hInc} * Return z z
 in k' s'))) z x

83.
{-- E-CaseRight --}

\ z. handler{hExcept} * \ zs. do z <- (Fst zs
 in do s' <- (Snd zs
 in do k' <- (\ z. handler{hInc} * Return z z
 in k' s'))) z ("fail", 9)

84.
{-- E-AppAbs --}

handler{hExcept} * \ zs. do z <- (Fst zs
 in do s' <- (Snd zs
 in do k' <- (\ z. handler{hInc} * Return z z
 in k' s'))) ("fail", 9)

85.
{-- E-Hand and E-AppAbs --}

handler{hExcept} * do z <- (Fst ("fail", 9)
 in do s' <- (Snd ("fail", 9)
 in do k' <- (\ z. handler{hInc} * Return z z
 in k' s')))

86.
{-- E-Hand and E-Do and E-Unop --}

handler{hExcept} * do z <- (Return "fail"
 in do s' <- (Snd ("fail", 9)
 in do k' <- (\ z. handler{hInc} * Return z z
 in k' s')))

87.
{-- E-Hand and E-DoRet --}

handler{hExcept} * do s' <- (Snd ("fail", 9)
 in do k' <- (\ z. handler{hInc} * Return z "fail"
 in k' s'))

88.
{-- E-Hand and E-Do and E-Unop --}

handler{hExcept} * do s' <- (Return 9
 in do k' <- (\ z. handler{hInc} * Return z "fail"
 in k' s'))

89.
{-- E-Hand and E-DoRet --}

handler{hExcept} * do k' <- (\ z. handler{hInc} * Return z "fail"
 in k' 9)

90.
{-- E-Hand and E-Do and E-AppAbs --}

handler{hExcept} * do k' <- (handler{hInc} * Return "fail"
 in k' 9)

91.
{-- E-Hand and E-Do and E-HandRet --}

handler{hExcept} * do k' <- (Return \ s. Return ("fail", s)
 in k' 9)

92.
{-- E-Hand and E-DoRet --}

handler{hExcept} * \ s. Return ("fail", s) 9

93.
{-- E-Hand and E-AppAbs --}

handler{hExcept} * Return ("fail", 9)

94.
{-- E-HandRet --}

Return Right ("fail", 9)

95.
{-- Nothing --}

